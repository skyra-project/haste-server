{
	"name": "haste-server",
	"version": "1.0.0",
	"description": "Private Pastebin Server",
	"private": true,
	"scripts": {
		"start": "node src/backend/server.js",
		"dev": "yarn build && yarn start",
		"build": "parcel build src/frontend/index.html && node scripts/copyRobots.mjs",
		"clean": "rimraf \".parcel-cache\" \"dist\" \"data\"",
		"lint": "eslint src scripts --fix --ext js,ts,mjs",
		"format": "prettier --write src scripts"
	},
	"dependencies": {
		"@pnotify/core": "^5.2.0",
		"connect": "^3.7.0",
		"connect-ratelimit": "0.0.7",
		"connect-route": "0.1.5",
		"highlight.js": "^11.6.0",
		"ioredis": "^5.2.3",
		"regenerator-runtime": "^0.13.9",
		"st": "^3.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@parcel/packager-raw-url": "2.5.0",
		"@parcel/packager-xml": "2.5.0",
		"@parcel/transformer-image": "2.5.0",
		"@parcel/transformer-sass": "2.5.0",
		"@parcel/transformer-webmanifest": "2.5.0",
		"@parcel/transformer-xml": "2.5.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"parcel": "2.5.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.4"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.6"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.3"
}
