{
	"name": "haste-server",
	"version": "1.0.0",
	"description": "Private Pastebin Server",
	"private": true,
	"scripts": {
		"start": "node src/backend/server.js",
		"dev": "yarn build && yarn start",
		"build": "parcel build src/frontend/index.html && node scripts/copyRobots.mjs",
		"clean": "rimraf \".parcel-cache\" \"dist\" \"data\"",
		"lint": "eslint src scripts --fix --ext js,ts,mjs",
		"format": "prettier --write src scripts"
	},
	"dependencies": {
		"@pnotify/core": "^5.2.0",
		"connect": "^3.7.0",
		"connect-ratelimit": "0.0.7",
		"connect-route": "0.1.5",
		"highlight.js": "^11.4.0",
		"ioredis": "^4.28.3",
		"regenerator-runtime": "^0.13.9",
		"st": "^3.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@parcel/packager-raw-url": "^2.2.0",
		"@parcel/packager-xml": "^2.2.0",
		"@parcel/transformer-image": "^2.2.0",
		"@parcel/transformer-sass": "^2.2.0",
		"@parcel/transformer-webmanifest": "^2.2.0",
		"@parcel/transformer-xml": "^2.2.0",
		"@sapphire/eslint-config": "^4.0.9",
		"@sapphire/prettier-config": "^1.2.8",
		"@sapphire/ts-config": "^3.1.7",
		"@types/ioredis": "^4.28.7",
		"eslint": "~8.6.0",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.2.0",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.5.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.1.1"
}
